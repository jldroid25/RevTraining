
public class ProgramTaskList {

	// write a program to repeat a multiple of a number 
	// 5, - 3, 10, 15 ...
	
	public static void repeatMultiple(int num  ) {
		for(int i =  0;  i <= 50 ; i++  ) {
			int result  =  i * num;
	
			System.out.println( "Here multiples of a number : " + result );
		}
	}
	
	public static void addSum() {
		int result = 2;
		
		for(int a =  0;  a <= 10 ; a++  ) {
			result += a ;
			System.out.println( "adding numbers  : " +  result );
		}
	}
	
	public static void main(String[] args) {
		// TODO Auto-generated method stub
		 repeatMultiple(5);
		 
		 System.out.println( "----------------------------------------");
		 
		 //addSum();
		 
	}//main 
}//class
