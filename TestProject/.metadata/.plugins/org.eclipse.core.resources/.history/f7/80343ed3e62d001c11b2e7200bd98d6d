
public class ProgramTaskList {

	// write a program to repeat a multiple of a number 
	// 5, - 3, 10, 15 ...
	
	public static void repeatMultiple() {
		for(int i =  2;  i  * 5  <= 50 ; i++  ) {
	
			System.out.println( "Here multiples of 5 : " + i  + "\n");
		}
	}
	
	public static void addSum() {
		int result = 2;
		
		for(int a =  0;  a <= 10 ; a++  ) {
			result += a ;
			System.out.println( "adding numbers  : " +  result + "\n");
		}
	}
	
	public static void main(String[] args) {
		// TODO Auto-generated method stub
		 repeatMultiple();
		 
		 System.out.println( "----------------------------------------");
		 
		 //addSum();
		 
	}//main 
}//class
