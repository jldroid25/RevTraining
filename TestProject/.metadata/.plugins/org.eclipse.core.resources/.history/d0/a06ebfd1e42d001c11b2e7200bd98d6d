
public class ProgramTaskList {

	// write a program to repeat a multiple of a number 
	// 5, - 3, 10, 15 ...
	
	public static void repeatMultiple() {
		for(int i =  5;  i <= 50 ; i++  ) {
			int result = i * 2; 
			System.out.println( "Here multiples of 5 : " + result +  "\n");
		}
	}
	
	public static void main(String[] args) {
		// TODO Auto-generated method stub
		 repeatMultiple();
	}//main 
}//class
