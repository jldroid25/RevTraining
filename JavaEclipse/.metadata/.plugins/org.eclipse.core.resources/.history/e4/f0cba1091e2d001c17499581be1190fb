
public class FirstDemo {

	public static void main(String[] args) {
		
		System.out.println("\n");

		//Here is a comment in Java
		System.out.println("\t\t Hello World from Eclipse IDE !");
		
		System.out.println("\n");
		
		System.out.println("\t\tThis is my string: " + myString);
		System.out.println("\t\tThis is my integer: " + myInt);

	}//main

}//class
